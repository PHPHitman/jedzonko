<+>{% extends 'base.html.twig' %}\r\n\r\n\r\n{% block body %}\r\n    <div class=\"body_container\">\r\n<div class=\"row\">\r\n\r\n    <div class=\"col-12 col-lg-5\">\r\n\r\n        <div class=\"sidebar\">\r\n            <table class=\"table\">\r\n\r\n                <thead class=\"thead-dark\">\r\n                <tr>\r\n                    <th scope=\"col\">Zamówienie</th>\r\n                    <th scope=\"col\">Należność</th>\r\n                    <th scope=\"col\">Działanie</th>\r\n                </tr>\r\n                </thead>\r\n\r\n\r\n                <tbody id=\"table\">\r\n\r\n\r\n                </tbody>\r\n                <tbody>\r\n                <tr>\r\n                    <td>W sumie</td>\r\n                    <td id=\"total_price\"></td>\r\n                </tr>\r\n                </tbody>\r\n\r\n            </table>\r\n\r\n            <button class=\"btn btn-info\" id=\"submit\" >Złóż zamówienie</button>\r\n        </div>\r\n\r\n        <div class=\"orders\">\r\n            <h2>Twoje dzisiejsze zamówienie</h2><hr/>\r\n            <table class=\"table\" id=\"orders_table\">\r\n\r\n                <thead class=\"thead-dark\">\r\n                <tr>\r\n                    <th scope=\"col\">Produkt</th>\r\n                    <th scope=\"col\">Cena</th>\r\n\r\n\r\n                </tr>\r\n                </thead>\r\n\r\n\r\n                <tbody id=\"orders\">\r\n\r\n\r\n                </tbody>\r\n\r\n            </table>\r\n\r\n            <div class=\"buttons\">\r\n                <button class=\"btn btn-warning\" id=\"edit_btn\">Edytuj</button>\r\n                <button class=\"btn btn-danger float-right delete_all\" id=\"delete_all\" >Usuń</button>\r\n            </div>\r\n\r\n\r\n        </div>\r\n\r\n    </div>\r\n    <div class=\"col-12 col-lg-7\">\r\n\r\n<div class=\"container\">\r\n    {{ block ('flashmessage', 'base.html.twig') }}\r\n    {% if is_granted('ROLE_ADMIN') %}\r\n\r\n\r\n        <div class=\"users_order\">\r\n            <h2>Dzisiejsze zamówienia:</h2><hr/>\r\n\r\n            <table class=\"table table-striped\" id=\"admin_order_table\">\r\n\r\n                <thead class=\"thead-dark\">\r\n                <tr>\r\n                    <th scope=\"col\">Użytkownik</th>\r\n                    <th scope=\"col\">Produkty</th>\r\n                    <th scope=\"col\">Cena</th>\r\n                    <th scope=\"col\">Status</th>\r\n                    <th scope=\"col\">Zmień</th>\r\n\r\n                </tr>\r\n                </thead>\r\n\r\n                <tbody id=\"user_orders\">\r\n\r\n                </tbody>\r\n\r\n            </table>\r\n\r\n            <div class=\"buttons\">\r\n\r\n            </div>\r\n        </div>\r\n\r\n    {% endif %}\r\n\r\n\r\n\r\n\r\n\r\n<div class=\"category\">\r\n    <div class=\"row\">\r\n        <div class=\"col col-xl-4\">\r\n            <button class=\" btn btn-info category_card\" id=\"Rzeszowskie kulinaria\">Rzeszowskie kulinaria</button>\r\n        </div>\r\n        <div class=\"col col-xl-4\">\r\n            <button class=\" btn btn-info category_card\" id=\"Burger point>Burger Point\">Burger point</button>\r\n        </div>\r\n        <div class=\"col col-xl-4\">\r\n            <button class=\" btn btn-info category_card\" id=\"Volcano\">Volcano</button>\r\n        </div>\r\n        <div class=\"col col-xl-4\">\r\n            <button class=\" btn btn-info category_card\" id=\"Dersim\">Dersim</button>\r\n        </div>\r\n\r\n    </div>\r\n</div>\r\n\r\n        <div class=\"searchbox\">\r\n            <div class=\"row\">\r\n            <div class=\"col-6\">\r\n            <span id=\"value\">Wyszukaj produkt</span>\r\n\r\n            <input id=\"searchbox\"type=\"text\" placeholder=\"wprowadź nazwę\">\r\n            </div>\r\n            <div class=\"col-6\">\r\n                <div id=\"searchedfood\">\r\n{#                    <a href=\"\" id=\"foodSearchHref\">#}\r\n{#                      <img class=\"foodSearchImg\">#}\r\n{#                    </a>#}\r\n\r\n                </div>\r\n                <div class=\"describe\">\r\n                    <span class=\"name\"></span><br/>\r\n                    <span class=\"price\"></span>\r\n                </div>\r\n            </div>\r\n\r\n</div>\r\n        </div>\r\n<div class=\"food_container\">\r\n\r\n            <div class=\"content\" id=\"dania\">\r\n                <h2>Dania</h2>\r\n\r\n\r\n                <section class=\"food\" id=\"dania\">\r\n\r\n{#                    {% for food in foods %}#}\r\n{#                        <div class=\"product\" id=\"{{ food.company }}\">#}\r\n{#                        <div>#}\r\n\r\n{#                                <div class=\"img\">#}\r\n{#                                <img src=\"{{'/uploads/' ~ food.image}}\" class=\"picture\" data-product_id=\"{{ food.id }}\" id=\"Rzeszowskiekulinaria\">#}\r\n{#                                <span>{{ food.name}}</span><br/><span> {{  food.price ~' zł'}}</span>#}\r\n{#                                </div>#}\r\n{#                            </div>#}\r\n\r\n{#                        </div>#}\r\n{#                    {% endfor %}#}\r\n\r\n                </section>\r\n\r\n            </div>\r\n\r\n\r\n\r\n\r\n\r\n            <div class=\"content\" id=\"desery\">\r\n                <h2>Desery</h2>\r\n\r\n                <section class=\"food\">\r\n\r\n                    {% for dessert in desserts %}\r\n                        <div id=\"dessert\">\r\n\r\n{#                            <img src=\"{{'/uploads/' ~ dessert.image}}\" class=\"picture\"  data-product_id=\"{{ dessert.id }}\">#}\r\n{#                            <span>{{ dessert.name }}</span>#}\r\n\r\n\r\n                        </div>\r\n                    {% endfor %}\r\n\r\n                </section>\r\n\r\n            </div>\r\n\r\n\r\n            <div class=\"content\" id=\"napoje\">\r\n                <h2>Napoje</h2>\r\n\r\n                <section class=\"food\">\r\n\r\n                    {% for drink in drinks %}\r\n                        <div>\r\n\r\n                            <img src=\"{{'/uploads/' ~ drink.image}}\" class=\"picture\" data-product_id=\"{{ drink.id }}\">\r\n                            <span>{{ drink.name }}</span>\r\n\r\n                        </div>\r\n                    {% endfor %}\r\n\r\n                </section>\r\n            </div>\r\n    </div>\r\n</div>\r\n    </div>\r\n</div>\r\n    </div>\r\n\r\n    {% block javascripts %}\r\n    {{ encore_entry_script_tags('app') }}\r\n    {% endblock %}\r\n\r\n{% endblock %}
